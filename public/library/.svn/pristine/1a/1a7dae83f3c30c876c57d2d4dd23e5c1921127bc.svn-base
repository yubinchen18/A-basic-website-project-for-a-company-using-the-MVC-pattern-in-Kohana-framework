	background-image: url(../img/bg_columnnav_a_active.png);
}

.columnNav ul li.active a:hover { color: #fff; }

.columnNav ul li a:active{
	position: relative;
	top: 1px;
}

.columnNav ul li a:hover { color: #000; }

