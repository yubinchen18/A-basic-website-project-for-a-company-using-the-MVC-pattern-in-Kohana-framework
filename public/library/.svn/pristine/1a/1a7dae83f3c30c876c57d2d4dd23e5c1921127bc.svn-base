	border-top: 1px solid #bdbdbd;
	overflow: hidden;
	position: relative;
}

.columnNav ul li:last-child {
	-webkit-border-bottom-right-radius: 10px;
	-webkit-border-bottom-left-radius: 10px;
	-moz-border-radius-bottomright: 10px;
	-moz-border-radius-bottomleft: 10px;
	border-bottom-right-radius: 10px;
	border-bottom-left-radius: 10px;
}

.columnNav ul li:first-child {
	border-top: 0px;
	
	-webkit-border-top-left-radius: 10px;
	-webkit-border-top-right-radius: 10px;
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-topright: 10px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

.columnNav ul li:hover {
	background: #CDCDCD; /*old IE fallback*/
	background: rgba(156, 156, 156, 0.3);	
}

.columnNav ul li.active,
.columnNav ul li.active:hover {
	border-bottom: 1px solid #3e92e8;
	background: url(../img/bg_columnnav_li_active.png) top left repeat-x; /* geen CSS gradient ivm border radius in IE*//* geen CSS gradient ivm border radius in IE*/
}

.columnNav ul li a{
	color: #505050;
	text-decoration: none;
	font-size: 12px;
	display: block;
	height: 100%;
	padding: 0px 10px;
	line-height: 40px;
	background-image: url(../img/bg_columnnav_a.png);
	background-position: 190px 16px;
	background-repeat: no-repeat;
}

.columnNav ul li a.create_branch,
.columnNav ul li a.create,
.columnNav ul li a.create_folder,
