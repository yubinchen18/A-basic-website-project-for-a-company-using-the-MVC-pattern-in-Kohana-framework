/*Reset*/
* {
	margin: 0px;
	padding: 0px;
	outline: none;
	outline-style: none;
	/* test: als er dingen kapot gaan met paddings moet dit er weer uit*/
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

a {
	outline: 0px;
	border: 0px;
}

html {
	font-family: arial, helvetica, verdana;
	font-size: 75%;
	cursor: default;
	line-height: 150%;
	min-width: 1024px;
}

body {
	background: #e8e8e8 url(../img/bg_body.gif);
}

.floatRight { float: right!important; }
.floatLeft { float: left!important; }
.clearBoth { clear: both!important; }
.hide { display: none!important; }

/* */

#sectionNavContainer {
	height: 95px;
	background: #505050;
	border-bottom: 1px solid #f9f9fb;
}

#sectionNavInner { }

#sectionNav {
	float: left;
	padding-left: 15px;
}

#topNav {
	float: right;
	text-align: right;
	margin-right: 15px;
	margin-top: 10px;
	color: #fff;
	padding: 3px 15px;
	font-size: 12px;
	-webkit-border-radius: 8px;
	-moz-border-radius: 8px;
	border-radius: 8px;
}

div.topNavItem {
	float: left;
	margin-left: 15px;
}

#moduleNavContainer {
	height: 45px;
	border-top: 1px solid #e3e3e3;
	border-bottom: 1px solid #9f9f9f;
	padding-left: 15px;	
	
	/*Gradient*/
	background: #e9e9e9; /* Old browsers */
	/* IE9 SVG, needs conditional override of 'filter' to 'none' */
	background: url(data:image/svg+xml;base64,);
	background: -moz-linear-gradient(top,  #e9e9e9 0%, #cdcdcd 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#e9e9e9), color-stop(100%,#cdcdcd)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top,  #e9e9e9 0%,#cdcdcd 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top,  #e9e9e9 0%,#cdcdcd 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top,  #e9e9e9 0%,#cdcdcd 100%); /* IE10+ */
	background: linear-gradient(top,  #e9e9e9 0%,#cdcdcd 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#e9e9e9', endColorstr='#cdcdcd',GradientType=0 ); /* IE6-8 *//* IE6-8 */
}

#mainContainer {
	background: url(../img/bg_main.png) top left repeat-x;
	padding: 25px 0px 0px 15px;
}

/*Column Structure*/
.contentRow {
	width: 960px;
	clear: both;	
}

.contentRow .column {
	float: left;
	display: inline-block;
	margin-right: 15px;
}

.columnNav { width: 210px; }

.columnContent { width: 720px; }

.columnContent h2 { float: left; }

div.collection h2 { float: none; }

ul.items,
ul.items a {
	margin-bottom: 10px;
	color: #1f8fc6;
	verflow: hidden; /*eraf voor de imagetools*/
	/*
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 10px;
	*/
	text-decoration: none;
}

ul.items a:hover {
	color: #000!important;
}

/*
ul.items a{
	color: #1f8fc6;
}
*/

ul.items li {
	padding: 10px 0px 8px 0px;
	position: relative;
	verflow: hidden; /*eraf voor de tools in de image lijst*/
	background: #fff;
	loat: left;
}



ul.items li.type_row {
	verflow: hidden;
}


ul.items li.depth_even { border-top: 1px solid #d1d1d1; }

ul.items li.depth_odd { border-top: 1px solid #fff; }

ul.items li span { line-height: 160%; }

ul.items li div.type_item,
ul.items li div.type_branch {
	display: inline-block;
	padding-left: 20px;
	min-height: 18px;
}

ul.items li div.type_branch {
	background: url(../img/icon_branch.png) 4px center no-repeat;
	ackground: url(../img/icon_item.png) 4px center no-repeat;
}

ul.items li div.type_branch { min-height: 20px; }

ul.items ul li.depth_odd { background: #e6e6e6; }

ul.items ul li.active.depth_odd {
	background: rgba(230, 230, 230, 0.5);
}

ul.items li:hover span.tools { }

ul.items li.active {
	ackground: #f1f1ff!important;
	background: rgba(255, 255, 255, 0.5);
}

ul.items li.type_row:first-child {
	border-top: 0px;
	
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-topright: 10px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

ul.items li.type_row:last-child {

	-moz-border-radius-bottomright: 10px;
	-moz-border-radius-bottomleft: 10px;

	border-bottom-left-radius:10px;
	border-bottom-right-radius:10px;
}

span.icon_link {
	padding-left: 20px;
	background: url(../img/icon_link.png) left center no-repeat;
}

span.icon_placeholder {
	padding-left: 20px;
	background: url(../img/icon_placeholder.png) left center no-repeat;
}


ul.draggable li {
	cursor: move;
}

ul.items li span.tools {
	display: none;
	padding-right: 10px;
	margin-right: 10px;
	position: absolute;
	right: 0px;
	top: 6px;
	
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
}


ul.items li div.itemContent {
	width: 450px;
	display: inline-block;
	/*
	display: inline;
    float: left;
    width: 430px;
	*/
	float: left;
}

div.type_branch div.itemContent span.title {
	font-weight: bold;
}

ul.items li span.tools a {
	margin-left: 5px;
	padding-left: 20px;
	line-height: 150%;
	color: #777777;
	text-decoration: none;
	position: relative;
	right: 0px;
	line-height: 25px;

}

li span.tools { background: #e9e9e9; }

li.active span.tools { background: #fff; }

li span.tools.depth_odd { background: #fff; }

li span.tools a { order-left: 1px solid #fff; }

span.tools.depth_odd a { order-left: 1px solid #e9e9e9; }

li span.tools a:first-child { border-left: none; }

ul.items li span.tools a:hover { color: #000; }

ul.items li a:active {
	position: relative;
	top: 1px;
}

/* tool styling*/
span.tools a {
	background-position: 2px center;
	background-repeat: no-repeat;
}

span.tools a.tool_update_branch,
span.tools a.tool_update { background-image: url(../img/icon_tools_update.png); }

span.tools a.tool_delete_branch,
span.tools a.tool_delete { background-image: url(../img/icon_tools_delete.png); background-position: 5px center; }

span.tools a.tool_hide { background-image: url(../img/icon_tools_hide.png); }

span.tools a.tool_show { background-image: url(../img/icon_tools_show.png); }

span.tools a.tool_view_image,
span.tools a.tool_view_file { background-image: url(../img/icon_tools_view.png); }

span.tools a.tool_roles { background-image: url(../img/icon_tools_roles.png); }

span.tools a.tool_preview { background-image: url(../img/icon_tools_preview.png); }

span.tools a.tool_use { background-image: url(../img/icon_tools_use.png); }

span.tools a.tool_version { background-image: url(../img/icon_tools_version.png); }

span.tools a.tool_revert { background-image: url(../img/icon_tools_revert.png); }

/*list state icon*/
span.state,
span.status,
span.imported { float: left; }

span.state_visible_past,
span.state_visible_future,
span.state_visible_current,
span.status_edit,
span.imported {
	width: 20px;
	height: 18px;
}

li span.state,
li span.status,
span.imported {
	background-position: left center;
	background-repeat: no-repeat;
}

span.state				 			{ background-image: url(../img/bg_status_invisible.png); }
span.state_visible_current		{ background-image: url(../img/bg_state_visible.png); }
li span.state_visible_past		{ background-image: url(../img/bg_state_visible_past.png); }
li span.state_visible_future	{ background-image: url(../img/bg_state_visible_future.png); }
li span.imported					{ background-image: url(../img/icon_google_imported.png); }

span.status_edit { background-image: url(../img/bg_status_invisible.png); }

span.status_edit~span.title,
span.status_edit~span.title a,
span.state_visible_past~span.title,
span.state_visible_past~span.title a,
span.state_visible_future~span.title,
span.state_visible_future~span.title a { color: #9a9a9a; }

span.error {
	width: 16px;
	height: 16px;
	background: red;
	display: inline-block;
	top: 4px;
	position: relative;
	margin-right: 2px;
	background: url(../img/icon_exclamation.png) top left no-repeat;
}

/* list style version element */
ul.items li span.revision strong {
	display: inline-block;
	background: #9F9F9F;
	padding: 2px 5px;
	color: white;
		-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	border-radius: 3px;
	float: left;
	margin-right: 10px			
}

ul.items li.type_row:last-child span.revision strong
{
	background: #1f8fc6;
}

ul.items li span.revision em {
 color: gray;
    display: block;
    float: left;
    font-size: 92%;
    line-height: 11px;
    margin-top: 0;
    width: 75px;
}



/* list style title element */
ul.items li.type_tile {
	width: 110px;
	height: 110px;
	padding: 0px;
	float: left;
	margin-right: 10px;
	margin-bottom: 8px;
	display: inline-block;
	border: none;
}

li.type_tile:first-child { }

li.type_tile span.tools { display: none; }

ul.items li.type_tile span.tools {
	background: url(../img/bg_imagetools.png);
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	-moz-box-shadow: 		2px 2px 3px 0px rgba(0,0,0,0.4);
	-webkit-box-shadow: 	2px 2px 3px 0px rgba(0,0,0,0.4);
	box-shadow: 			2px 2px 3px 0px rgba(0,0,0,0.4);
}

li.type_tile span.tools a {
	color: #7a7a7a;
}

li.type_tile span.tools span.tools_left {
	position: absolute;
	left: -12px;
	top: 35px;
	width: 12px;
	height: 24px;
	background: url(../img/bg_imagetools_left.png) top left no-repeat;
}

ul.items li.type_tile div.type_item { padding: 10px; }

ul.items li.type_tile div.itemContent {
	position: relative;
	width: 90px;
	height: 90px;
	background: url(../img/bg_itemtile_thumb.gif) top left repeat;
	text-align: center;
}

ul.items li.type_tile div.itemContent img {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
}

ul.items li.type_tile span.title { display: none; }	

ul.items li.type_tile span.tools {
	position: absolute;
	left: 80px;
	op: -15px;
	width: 90px;
	padding: 10px 0px 0px 0px;
	z-index: 100;
	/*	top: -moz-calc(100%);*/
	top: 10px;
}

ul.items li.type_tile span.tools a{
	clear: both;
	display: block;
	border: none;
	order-bottom: 1px solid #fff;
	line-height: 15px;
	height: 17px;
}

/*Default styling*/
h2 {
	color: #6a6565;
