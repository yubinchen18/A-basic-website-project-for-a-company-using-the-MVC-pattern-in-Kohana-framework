/*collectionItem*/
div.collection {
	display: block;
	clear: both;
	margin-bottom: 20px;
	float: left;
	width: 100%;
}

div.collection h3 {
	margin-bottom: 5px;
	color: #606060;
}

.image .collectionItem,
.image .connectionItem {
	float: left;
	width: 90px;
	height: 90px;
	background: url(../img/bg_itemtile_thumb.gif) top left repeat;
	text-align: center;
	margin-right: 10px;
	margin-bottom: 10px;
	position: relative;
}

.image .collectionItem img,
.image .connectionItem img {
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
}

.image .collectionItem .collectionItemRemove,
.image .connectionItem .connectionItemRemove {
	position: absolute;
	top: 65px;
	right: 4px;
	width: 16px;
	height: 16px;
	background: url(../img/bg_collection_item_remove.png)top left no-repeat;
}

.image .collectionItem .collectionItemRemove span,
.image .connectionItem .connectionItemRemove span { display: none; }

.image .collectionAdd,
.image .connectionAdd {
	float: left;
	width: 90px;
	height: 90px;
	background: #dadada url(../img/btn_add.png) 70px 70px no-repeat;
	
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
}

.image .collectionAdd span,
.image .connectionAdd span { display: none; }

.ui-sortable .collectionItem { cursor: move; }

.image .collectionItems {
	order: 1px solid green;
	float: left;
}

.file div.collectionItems .collectionItem,
.item div.collectionItems .collectionItem,
.item .connectionContainer .connectionItem {
	ackground: #e6e6e6;
	argin-bottom: 1px;
	position: relative;
	height: 35px;
	line-height: 35px;
	padding-left: 20px;
