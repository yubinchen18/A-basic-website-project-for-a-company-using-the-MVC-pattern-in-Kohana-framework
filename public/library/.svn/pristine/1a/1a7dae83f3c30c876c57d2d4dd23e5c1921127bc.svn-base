.item .collectionAdd,
.item .connectionAdd {
	display: block;
	background: url(../img/btn_add.png) 617px 10px no-repeat;
	height: 35px;
	line-height: 35px;
	padding-left: 20px;
	color: #3b90e6;
	text-decoration: none;
}

.collectionlistBox,
.connection {
	background: #e6e6e6;
	
	-webkit-border-radius: 10px;
	-moz-border-radius: 10px;
	border-radius: 10px;
}

.collectionlistBox a:active {
	position: relative;
	top: 1px;
}

.file a.collectionAdd:first-child,
.item a.collectionAdd:first-child {
	-moz-border-radius-topleft: 10px;
	-moz-border-radius-topright: 10px;
	border-top-left-radius: 10px;
	border-top-right-radius: 10px;
}

.file  a.collectionItemRemove,
.item  a.collectionItemRemove,
.item a.connectionItemRemove {
	position: absolute;
	top: 10px;
	right: 10px;
	width: 16px;
	height: 16px;
	background: url(../img/bg_collection_item_remove.png)top left no-repeat;
}

.file  a.collectionItemRemove span,
.item  a.collectionItemRemove span,
.item  a.connectionItemRemove span
 { display: none; }

